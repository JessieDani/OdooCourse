<odoo>
    <template id="cesumin_sel_report_doc">
        <t t-call="web.external_layout">
            <h2 t-field="o.name" />
            <t t-if="o.log_ids">
                <h3>Logs</h3>
                <ul>
                    <t t-foreach="o.log_ids" t-as="log">
                        <li>
                            <span t-field="log.datetime"/>
                        </li>
                    </t>
                </ul>
            </t>
        </t>
    </template>


</odoo>